<manifest xmlns:android="http://schemas.android.com/apk/res/android" android:versionCode="1" android:versionName="1.0" package="com.dewey.jsdroidy">

    <uses-sdk android:minSdkVersion="7" android:targetSdkVersion="16"/>

    <application android:allowBackup="true" android:icon="@drawable/ic_launcher" android:label="@string/app_name" android:theme="@style/AppTheme">
        <activity android:label="@string/app_name" android:name="com.dewey.jsdroidy.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>



        <activity android:name="com.dewey.jsdroidy.output" android:parentActivityName="com.dewey.jsdroidy.MainActivity">
            <meta-data android:name="android.support.PARENT_ACTIVITY" android:value="com.dewey.jsdroidy.MainActivity"/>
        </activity>


    </application>

    <uses-permission android:name="android.permission.INTERNET"/>

</manifest>
